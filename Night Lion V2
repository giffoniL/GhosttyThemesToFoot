## Normal/regular colors (color palette 0-7)
regular0=4c4c4c  # black
regular1=bb0000  # red
regular2=04f623  # green
regular3=f3f167  # yellow
regular4=64d0f0  # blue
regular5=ce6fdb  # magenta
regular6=00dadf  # cyan
regular7=bbbbbb  # white

