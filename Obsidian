[colors]

# cursor-color and cursor-text
cursor=c0cad0 cdcdcd

background=283033
foreground=cdcdcd

selection-background=3e4c4f
selection-foreground=dfe1e2

## Normal/regular colors (color palette 0-7)
regular0=000000  # black
regular1=b30d0e  # red
regular2=00bb00  # green
regular3=fecd22  # yellow
regular4=3a9bdb  # blue
regular5=bb00bb  # magenta
regular6=00bbbb  # cyan
regular7=bbbbbb  # white

## Bright colors (color palette 8-15)
bright0=555555   # bright black
bright1=ff0003   # bright red
bright2=93c863  # bright green
bright3=fef874  # bright yellow
bright4=a1d7ff  # bright blue
bright5=ff55ff  # bright magenta
bright6=55ffff  # bright cyan
bright7=ffffff  # bright white
