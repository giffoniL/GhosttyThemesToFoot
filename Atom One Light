## Normal/regular colors (color palette 0-7)
regular0=000000  # black
regular1=de3e35  # red
regular2=3f953a  # green
regular3=d2b67c  # yellow
regular4=2f5af3  # blue
regular5=950095  # magenta
regular6=3f953a  # cyan
regular7=bbbbbb  # white

## Bright colors (color palette 8-15)
bright0=000000   # bright black
