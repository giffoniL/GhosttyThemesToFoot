[colors]

# cursor-color and cursor-text
cursor=ffffff 000000

background=000000
foreground=ffffff

selection-background=b5d5ff
selection-foreground=000000

## Normal/regular colors (color palette 0-7)
regular0=878787  # black
regular1=ff6600  # red
regular2=ccff04  # green
regular3=ffcc00  # yellow
regular4=44b4cc  # blue
regular5=9933cc  # magenta
regular6=44b4cc  # cyan
regular7=f5f5f5  # white

## Bright colors (color palette 8-15)
bright0=555555   # bright black
bright1=ff0000   # bright red
bright2=00ff00  # bright green
bright3=ffff00  # bright yellow
bright4=0000ff  # bright blue
bright5=ff00ff  # bright magenta
bright6=00ffff  # bright cyan
bright7=e5e5e5  # bright white
