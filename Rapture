selection-background=304b66
selection-foreground=ffffff

## Normal/regular colors (color palette 0-7)
regular0=000000  # black
regular1=fc644d  # red
regular2=7afde1  # green
regular3=fff09b  # yellow
regular4=6c9bf5  # blue
regular5=ff4fa1  # magenta
regular6=64e0ff  # cyan
regular7=c0c9e5  # white

## Bright colors (color palette 8-15)
bright0=304b66   # bright black
