## Normal/regular colors (color palette 0-7)
regular0=000000  # black
regular1=b12424  # red
regular2=006b4f  # green
regular3=f8a300  # yellow
regular4=138af2  # blue
regular5=9a1b6e  # magenta
regular6=3eb8bf  # cyan
regular7=bfbfbf  # white

