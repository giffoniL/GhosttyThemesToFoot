## Normal/regular colors (color palette 0-7)
regular0=000000  # black
regular1=aa3731  # red
regular2=448c27  # green
regular3=cb9000  # yellow
regular4=325cc0  # blue
regular5=7a3e9d  # magenta
regular6=0083b2  # cyan
regular7=b7b7b7  # white

