[colors]

# cursor-color and cursor-text
cursor=ffc663 141515

background=141515
foreground=ffffff

selection-background=515151
selection-foreground=ffc663

## Normal/regular colors (color palette 0-7)
regular0=333333  # black
regular1=d23e08  # red
regular2=54ca74  # green
regular3=fff700  # yellow
regular4=2ab9ff  # blue
regular5=ff50da  # magenta
regular6=1ef9f5  # cyan
regular7=ddd0c4  # white

## Bright colors (color palette 8-15)
bright0=515151   # bright black
bright1=ffb7b7   # bright red
bright2=c1ffae  # bright green
bright3=fcffb8  # bright yellow
bright4=8efff3  # bright blue
bright5=ffa2ed  # bright magenta
bright6=bcffc7  # bright cyan
bright7=ffffff  # bright white
