## Normal/regular colors (color palette 0-7)
regular0=000000  # black
regular1=b87a7a  # red
regular2=7ab87a  # green
regular3=b8b87a  # yellow
regular4=7a7ab8  # blue
regular5=b87ab8  # magenta
regular6=7ab8b8  # cyan
regular7=d9d9d9  # white

## Bright colors (color palette 8-15)
bright0=4c4c4c   # bright black
