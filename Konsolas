[colors]

# cursor-color and cursor-text
cursor=c8c1c1 060606

background=060606
foreground=c8c1c1

selection-background=060606
selection-foreground=c8c1c1

## Normal/regular colors (color palette 0-7)
regular0=000000  # black
regular1=aa1717  # red
regular2=18b218  # green
regular3=ebae1f  # yellow
regular4=2323a5  # blue
regular5=ad1edc  # magenta
regular6=42b0c8  # cyan
regular7=c8c1c1  # white

## Bright colors (color palette 8-15)
bright0=7b716e   # bright black
bright1=ff4141   # bright red
bright2=5fff5f  # bright green
bright3=ffff55  # bright yellow
bright4=4b4bff  # bright blue
bright5=ff54ff  # bright magenta
bright6=69ffff  # bright cyan
bright7=ffffff  # bright white
