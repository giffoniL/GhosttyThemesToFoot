## Normal/regular colors (color palette 0-7)
regular0=000000  # black
regular1=ff4d83  # red
regular2=1f8c3b  # green
regular3=1fc95b  # yellow
regular4=1dd3ee  # blue
regular5=8959a8  # magenta
regular6=3e999f  # cyan
regular7=bfbfbf  # white

