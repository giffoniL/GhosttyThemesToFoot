[colors]

# cursor-color and cursor-text
cursor=808080 ffffff

background=000000
foreground=f1f1f1

selection-background=b5d5ff
selection-foreground=000000

## Normal/regular colors (color palette 0-7)
regular0=4f4f4f  # black
regular1=fa6c60  # red
regular2=a8ff60  # green
regular3=fffeb7  # yellow
regular4=96cafe  # blue
regular5=fa73fd  # magenta
regular6=c6c5fe  # cyan
regular7=efedef  # white

## Bright colors (color palette 8-15)
bright0=7b7b7b   # bright black
bright1=fcb6b0   # bright red
bright2=cfffab  # bright green
bright3=ffffcc  # bright yellow
bright4=b5dcff  # bright blue
bright5=fb9cfe  # bright magenta
bright6=e0e0fe  # bright cyan
bright7=ffffff  # bright white
