## Normal/regular colors (color palette 0-7)
regular0=000000  # black
regular1=ff5370  # red
regular2=c3e88d  # green
regular3=ffcb6b  # yellow
regular4=82aaff  # blue
regular5=c792ea  # magenta
regular6=89ddff  # cyan
regular7=ffffff  # white

