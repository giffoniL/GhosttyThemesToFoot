[colors]

# cursor-color and cursor-text
cursor=c4c4c4 c4c4c4

background=000000
foreground=c4c4c4

selection-background=c4c4c4
selection-foreground=000000

## Normal/regular colors (color palette 0-7)
regular0=000000  # black
regular1=c40000  # red
regular2=00c400  # green
regular3=c47e00  # yellow
regular4=0000c4  # blue
regular5=c400c4  # magenta
regular6=00c4c4  # cyan
regular7=c4c4c4  # white

## Bright colors (color palette 8-15)
bright0=4e4e4e   # bright black
bright1=dc4e4e   # bright red
bright2=4edc4e  # bright green
bright3=f3f34e  # bright yellow
bright4=4e4edc  # bright blue
bright5=f34ef3  # bright magenta
bright6=4ef3f3  # bright cyan
bright7=ffffff  # bright white
