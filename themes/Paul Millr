## Normal/regular colors (color palette 0-7)
regular0=2a2a2a  # black
regular1=ff0000  # red
regular2=79ff0f  # green
regular3=e7bf00  # yellow
regular4=396bd7  # blue
regular5=b449be  # magenta
regular6=66ccff  # cyan
regular7=bbbbbb  # white

