## Normal/regular colors (color palette 0-7)
regular0=101010  # black
regular1=f5a191  # red
regular2=90b99f  # green
regular3=e6b99d  # yellow
regular4=aca1cf  # blue
regular5=e29eca  # magenta
regular6=ea83a5  # cyan
regular7=a0a0a0  # white

