selection-background=ff3399
selection-foreground=ffffff

## Normal/regular colors (color palette 0-7)
regular0=1d1d26  # black
regular1=ff3399  # red
regular2=00d364  # green
regular3=ffcc66  # yellow
regular4=00cbff  # blue
regular5=cc66ff  # magenta
regular6=00ceca  # cyan
regular7=cbcbf0  # white

