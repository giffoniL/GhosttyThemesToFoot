## Normal/regular colors (color palette 0-7)
regular0=000000  # black
regular1=ff9da4  # red
regular2=d1f1a9  # green
regular3=ffeead  # yellow
regular4=bbdaff  # blue
regular5=ebbbff  # magenta
regular6=99ffff  # cyan
regular7=ffffff  # white

