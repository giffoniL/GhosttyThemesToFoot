## Normal/regular colors (color palette 0-7)
regular0=000000  # black
regular1=c91b00  # red
regular2=00c200  # green
regular3=c7c400  # yellow
regular4=2225c4  # blue
regular5=ca30c7  # magenta
regular6=00c5c7  # cyan
regular7=ffffff  # white

