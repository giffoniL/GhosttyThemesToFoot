selection-background=c2deff
selection-foreground=000000

## Normal/regular colors (color palette 0-7)
regular0=1a1919  # black
regular1=ff2b1e  # red
regular2=57ff3d  # green
regular3=ffff44  # yellow
regular4=0004ff  # blue
regular5=ff2cff  # magenta
regular6=56ffff  # cyan
regular7=ffffff  # white

