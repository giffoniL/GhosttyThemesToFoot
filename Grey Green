[colors]

# cursor-color and cursor-text
cursor=fff400 e1e4e3

background=002a1a
foreground=ffffff

selection-background=517e50
selection-foreground=e2e2e2

## Normal/regular colors (color palette 0-7)
regular0=000000  # black
regular1=fe1414  # red
regular2=74ff00  # green
regular3=f1ff01  # yellow
regular4=00deff  # blue
regular5=ff00f0  # magenta
regular6=00ffbc  # cyan
regular7=ffffff  # white

## Bright colors (color palette 8-15)
bright0=666666   # bright black
bright1=ff3939   # bright red
bright2=00ff44  # bright green
bright3=ffd100  # bright yellow
bright4=00afff  # bright blue
bright5=ff008a  # bright magenta
bright6=00ffd3  # bright cyan
bright7=f5ecec  # bright white
